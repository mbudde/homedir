# -*- sh-mode -*-

# hgit completion

source /usr/share/bash-completion/completions/git

__hgit ()
{
    GIT_DIR=$HOME/.homegit __git_main
}

__git_complete hgit __hgit

# rust
if [ -r ~/opt/rust-nightly/etc/bash_completion.d/cargo ]; then
    source ~/opt/rust-nightly/etc/bash_completion.d/cargo
fi
