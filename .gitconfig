[user]
	name = Michael Budde
	email = mbudde@gmail.com
[alias]
	st = status -sb
	di = diff -B
	co = checkout
	ci = commit
	br = branch
	h = help
	rb = rebase
	rt = remote
	pr = pull --rebase
	lg = log --stat --relative-date
	graph = log --graph --oneline --all --decorate
[color]
	diff = auto
	branch = auto
	status = auto
[color "diff"]
	meta = yellow
[core]
	excludesfile = ~/.gitignore
[diff]
	renames = copies
[gui]
	fontui = -family \"Bitstream Vera Sans\" -size 10 -weight normal -slant roman -underline 0 -overstrike 0
	fontdiff = -family \"Bitstream Vera Sans Mono\" -size 10 -weight normal -slant roman -underline 0 -overstrike 0
[url "git://git.gnome.org/"]
	insteadOf = gnome:
[url "git://github.com/"]
    insteadOf = github:
[url "git@github.com:"]
    insteadOf = githubp:
[push]
	default = matching
[github]
	user = mbudde
